{
"compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@App":["app.ts"],
      "@Commons/*": ["commons/*"],
      "@DB/*": ["database/*"],
      "@WorkSpace/*": ["modules/work.space.email/*"],
      "@EmailScheduler/*": ["modules/email.scheduler/*"]
    }
  }
}